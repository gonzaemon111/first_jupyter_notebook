    "\n",
    "a = np.array([0, 1, 2, 3, 4])\n",
    "b = np.array([2, 4, 6, 8, 10])\n",
    "\n",
    "\n",
    "print(\"a - b =\" + str(a - b)) # 先ほど作ったa,bを用いる。\n",
    "print(\"b - a =\" + str(b - a))\n",
    "\n",
    "print(\"a - 4 =\" + str(a - 4)) # 各々の要素から４ずつ引く。"
